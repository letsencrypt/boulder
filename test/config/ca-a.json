{
  "ca": {
    "debugAddr": ":8001",
    "tls": {
      "caCertFile": "test/grpc-creds/minica.pem",
      "certFile": "test/grpc-creds/ca.boulder/cert.pem",
      "keyFile": "test/grpc-creds/ca.boulder/key.pem"
    },
    "hostnamePolicyFile": "test/hostname-policy.yaml",
    "grpcCA": {
      "address": ":9093",
      "clientNames": [
