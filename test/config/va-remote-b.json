{
  "va": {
    "CAASERVFAILExceptions": "test/caa-servfail-exceptions.txt",
    "userAgent": "boulder-remote-b",
    "debugAddr": ":8012",
    "portConfig": {
      "httpPort": 5002,
      "httpsPort": 5001,
      "tlsPort": 5001
    },
    "dnsTries": 3,
    "dnsResolvers": [
      "127.0.0.1:8053",
      "127.0.0.1:8054"
    ],
    "issuerDomain": "happy-hacker-ca.invalid",
    "tls": {
      "caCertfile": "test/grpc-creds/minica.pem",
      "certFile": "test/grpc-creds/va.boulder/cert.pem",
      "keyFile": "test/grpc-creds/va.boulder/key.pem"
    },
    "grpc": {
      "address": ":9098",
      "clientNames": [
        "va.boulder"
      ]
    },
    "features": {
      "CAAValidationMethods": true,
      "CAAAccountURI": true
    },
    "accountURIPrefixes": [
      "http://boulder:4000/acme/reg/"
    ]
  },

  "syslog": {
    "stdoutlevel": 6,
    "sysloglevel": 4
  },

  "common": {
    "dnsTimeout": "1s",
    "dnsAllowLoopbackAddresses": true
  }
}
