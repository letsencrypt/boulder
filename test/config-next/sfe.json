{
	"sfe": {
		"listenAddress": "0.0.0.0:4003",
		"timeout": "30s",
		"shutdownStopTimeout": "10s",
		"tls": {
			"caCertFile": "test/certs/ipki/minica.pem",
			"certFile": "test/certs/ipki/sfe.boulder/cert.pem",
			"keyFile": "test/certs/ipki/sfe.boulder/key.pem"
		},
		"raService": {
			"dnsAuthority": "consul.service.consul",
			"srvLookup": {
				"service": "ra",
				"domain": "service.consul"
			},
			"timeout": "15s",
			"noWaitForReady": true,
			"hostOverride": "ra.boulder"
		},
		"saService": {
			"dnsAuthority": "consul.service.consul",
			"srvLookup": {
				"service": "sa",
				"domain": "service.consul"
			},
			"timeout": "15s",
			"noWaitForReady": true,
			"hostOverride": "sa.boulder"
		},
		"unpauseHMACKey": {
			"keyFile": "test/secrets/sfe_unpause_key"
		},
		"zendesk": {
			"baseURL": "http://localhost:9701",
			"tokenEmail": "admin@example.com",
			"token": {
				"passwordFile": "test/secrets/zendesk_api_token"
			},
			"customFields": {
				"Organization": 1234567,
				"Tier": 2345678,
				"RateLimit": 3456789,
				"ReviewStatus": 34567890,
				"AccountURI": 45678901,
				"RegisteredDomain": 56789012,
				"IPAddress": 67890123
			}
		},
		"overridesImporter": {
			"mode": "all",
			"interval": "20m"
		},
		"features": {}
	},
	"syslog": {
		"stdoutlevel": 4,
		"sysloglevel": -1
	},
	"openTelemetry": {
		"endpoint": "bjaeger:4317",
		"sampleratio": 1
	},
	"openTelemetryHttpConfig": {
		"trustIncomingSpans": true
	}
}
